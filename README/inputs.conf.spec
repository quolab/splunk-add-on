[quolab_timeline://]

server = <string>
* Name of QuoLab server
* Default: quolab
* (required)
timeline = <string>
* Timeline id from QuoLab
* (required)
backfill = <bool>
* If enabled, the first run will retrieve all existing events from the queue
* Default: True
* (optional)
log_level = <string>
* Logging level for internal logging
* Choices: DEBUG, INFO, WARN, ERROR
* Default: INFO
* (required)

disabled = <bool>
